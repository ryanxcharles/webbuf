{
  "name": "@webbuf/acb3dh",
  "version": "3.0.23",
  "description": "AES+CBC encryption/decryption with blake3 mac and secp256k1 Diffie-Hellman shared secret for web, node.js, deno and bun.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanxcharles/webbuf"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "benchmark": "vitest --config vitest.benchmark.config.ts run",
    "test": "vitest --run",
    "test:browser": "vitest --run --browser",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --write --unsafe",
    "format": "biome format --write",
    "fix": "pnpm run typecheck && pnpm run lint && pnpm run format",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "engines": {
    "node": ">=20.8.0"
  },
  "packageManager": "pnpm@9.7.0+sha256.b35018fbfa8f583668b2649e407922a721355cd81f61beeb4ac1d4258e585559",
  "keywords": [
    "buffer",
    "base64",
    "hex",
    "uint256",
    "int256",
    "uint128",
    "int128",
    "rust",
    "wasm"
  ],
  "author": "Ryan X. Charles <ryan@ryanxcharles.com> (https://ryanxcharles.com)",
  "license": "MIT",
  "peerDependencies": {
    "@webbuf/webbuf": "workspace:^",
    "@webbuf/fixedbuf": "workspace:^",
    "@webbuf/aescbc": "workspace:^",
    "@webbuf/blake3": "workspace:^",
    "@webbuf/acb3": "workspace:^",
    "@webbuf/secp256k1": "workspace:^"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.7.9",
    "buffer": "^6.0.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  }
}

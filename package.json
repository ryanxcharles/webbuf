{
  "name": "webbuf",
  "version": "2.0.2",
  "description": "A fork of \"buffer\" converted to ES Modules and with a different name.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanxcharles/webbuf"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "test:browser": "vitest --run --browser",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --write --unsafe",
    "format": "biome format --write",
    "fix": "pnpm run typecheck && pnpm run lint && pnpm run format",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [],
  "author": "Ryan X. Charles <ryan@ryanxcharles.com> (https://ryanxcharles.com)",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.7.9",
    "@vitest/browser": "^2.1.3",
    "buffer": "^6.0.3",
    "playwright": "^1.48.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  }
}
